<resources>

    <!--
            Base application theme, dependent on API level. This theme is replaced
            by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
        -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:popupMenuStyle">@android:style/Widget.Holo.Light.PopupMenu</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/fade</item>
    </style>

    <style name="fade" parent="@android:style/Animation.Translucent">
        <item name="android:activityOpenEnterAnimation">@anim/slide_in</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_out</item>
    </style>

    <style name="AppTheme.Light" parent="android:Theme.Holo.Light">
        <item name="android:windowBackground">@color/snowColor</item>
        <item name="android:actionBarStyle">@style/ActionBar_Light</item>
        <item name="android:actionOverflowButtonStyle">@style/OverflowButtonStyle_Light</item>
        <!--<item name="android:homeAsUpIndicator">@drawable/ic_title_up_default</item>-->
        <item name="android:homeAsUpIndicator">@drawable/ic_ab_back</item>
        <item name="android:actionBarTabStyle">@style/ActionBar_Light_TabStyle</item>
        <item name="android:actionBarTabTextStyle">@style/ActionBar_Light_TabTextStyle</item>
        <item name="android:actionBarTabBarStyle">@style/ActionBar_Light_TabBarStyle</item>
        <item name="android:textViewStyle">@style/RobotoTextViewStyle</item>
        <item name="android:buttonStyle">@style/RobotoButtonStyle</item>
        <item name="android:webTextViewStyle">@style/RobotoWebViewStyle</item>
        <item name="android:actionBarItemBackground">@drawable/actionbar_item_background_light</item>
    </style>

    <style name="tab_item">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">5dip</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:background">@drawable/home_btn_bg</item>

        <item name="android:textSize">10.0dip</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>


    <style name="ActionBar_Light" parent="android:Widget.Holo.ActionBar">
        <item name="android:titleTextStyle">@style/ActionBar_Light_TextStyle</item>
        <item name="android:background">@color/actionbar_background</item>
        <item name="android:backgroundSplit">@color/actionbar_background</item>
        <item name="android:displayOptions">homeAsUp|showTitle|showHome</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="ActionBar_Light_TextStyle" parent="android:TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="ActionBar_Light_TabStyle" parent="android:Widget.Holo.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_background</item>
    </style>

    <style name="ActionBar_Light_TabTextStyle" parent="android:Widget.Holo.ActionBar.TabText">
        <item name="android:textColor">@color/tab_text</item>
    </style>

    <style name="ActionBar_Light_TabBarStyle" parent="android:Widget.Holo.Light.ActionBar.TabBar">
        <item name="android:background">@color/ivoryColor</item>
    </style>

    <!-- Roboto font -->
    <style name="RobotoTextViewStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="RobotoButtonStyle" parent="android:Widget.Holo.Button">
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="RobotoWebViewStyle" parent="android:Widget.Holo.Light.WebTextView">
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="OverflowButtonStyle_Light" parent="android:Widget.Holo.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_title_overflow_default</item>
    </style>

    <style name="CommonListItemTitleText">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="CommonListItemInfoText">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#737373</item>
    </style>

    <style name="MyInfoTextLeft">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">right</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#c1c1c1</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="MyInfoTextRight">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
    </style>
    <style name="MyInfoTextRightMultiLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">45dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
    </style>



    <style name="LeftTextNormal">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#999999</item>
    </style>

    <style name="RightTextNormal">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#555555</item>
    </style>

    <style name="PopDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@animator/push_bottom_in</item>
        <item name="android:windowExitAnimation">@animator/push_bottom_out</item>
    </style>

    <style name="MyDialogStyleBottom" parent="android:Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>

        <item name="android:windowCloseOnTouchOutside">true</item>
        <!-- 模糊 -->
    </style>

    <!-- 导航条 Style -->
    <style name="Navbar">
        <item name="android:layout_height">45dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/news_titlebar_bg</item>
    </style>

    <style name="NavbarButtonContainer">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:minWidth">60dip</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingRight">10dip</item>
    </style>

    <style name="NavbarButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:textSize">@dimen/font_medium</item>
    </style>

    <style name="NavbarText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/font_big</item>
        <item name="android:drawablePadding">3dp</item>
    </style>

    <style name="vsButton">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/font_medium_2</item>
        <item name="android:background">@drawable/bg_contentlanjingblue_corner</item>
    </style>

    <!-- 正常文本样式：黑色字体，中等字号 -->
    <style name="NormalText">
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textSize">@dimen/font_medium</item>
    </style>

    <style name="SmallText" parent="NormalText">
        <item name="android:textSize">@dimen/font_small</item>
    </style>

    <style name="GrayText" parent="NormalText">
        <item name="android:textColor">@color/text_gray</item>
    </style>

    <style name="lanjingcontact_fensi_title" parent="NormalText">
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/font_small</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="lanjingcontact_fensi" parent="NormalText">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/font_small</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="seperateline">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>

        <item name="android:background">@color/light_gray_line</item>
    </style>

    <style name="interviewcontact_show_linearlayout">
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:orientation">horizontal</item>
    </style>


    <style name="interviewcontact_show_linearlayout1">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="interviewcontact_show_title">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">60dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_stardust</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textSize">@dimen/font_normal</item>
    </style>

    <style name="interviewcontact_show_title1">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">60dp</item>
        <item name="android:textColor">@color/text_stardust</item>
        <item name="android:gravity">right|top</item>
        <item name="android:textSize">@dimen/font_normal</item>
    </style>

    <style name="interviewcontact_show_content">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="interviewcontact_show_content1">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textSize">@dimen/font_medium</item>
    </style>

    <style name="interviewcontact_add_edittext">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textSize">@dimen/font_medium</item>
    </style>

    <style name="event_column">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/event_column</item>
        <item name="android:minHeight">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">20dp</item>
    </style>

    <style name="event_column_centent">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/event_column</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">20dp</item>
    </style>


    <style name="event_centent">
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="delete_event">
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:text">@string/delete_event</item>
        <item name="android:textColor">@color/event_column</item>
    </style>


    <style name="anim_popup_dir">
        <item name="android:windowEnterAnimation">@anim/slide_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>
    </style>

    <style name="Theme_Transparent" parent="android:Theme.Dialog">
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:windowContentOverlay">@null</item>
    <item name="android:windowNoTitle">true</item>
    </style>

    <style name="caterbookingTimeDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/caterbookingDialogAnimation</item>
    </style>


    <style name="caterbookingDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/in_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/out_to_bottom</item>
    </style>



    <style name="SystemMsgTabTextStyle">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="SearchFilterTextStyle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <!--透明dialog-->
    <style name="MyDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">@color/transparent</item>
    </style>
    <style name="NewDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:backgroundDimAmount">0</item>
    </style>
    <style name="AppThemeFullscreen" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="ShizhiTextAppearanceTab" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
        <item name="textAllCaps">false</item>
    </style>
    <style name="Widget.SeekBar.Normal" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:maxHeight">2dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/seekbar_horizontal</item>
        <item name="android:minHeight">2dip</item>
        <item name="android:thumb">@drawable/seek_thumb</item>
        <item name="android:thumbOffset">0dip</item>
    </style>
</resources>
