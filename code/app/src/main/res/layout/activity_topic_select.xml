<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:titlebar="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical">

  <com.ygs.android.yigongshe.view.CommonTitleBar
    android:id="@+id/titleBar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    titlebar:centerType="textView"
    titlebar:fillStatusBar="false"
    titlebar:leftType="imageButton"
    titlebar:rightCustomView="@layout/titlebar_right_text"
    titlebar:rightType="customView"></com.ygs.android.yigongshe.view.CommonTitleBar>

  <android.support.v7.widget.RecyclerView
    android:id="@+id/rv_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"></android.support.v7.widget.RecyclerView>

</LinearLayout>